# LiveKit Credentials (https://cloud.livekit.io/)
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# --- Local AI Stack ---
# Whisper STT (Docker: faster-whisper-server on port 8000)
WHISPER_BASE_URL=http://localhost:8000/v1
WHISPER_MODEL=tiny

# Kokoro TTS (Docker: kokoro-fastapi on port 8880)
KOKORO_BASE_URL=http://localhost:8880/v1
KOKORO_VOICE=af_heart
KOKORO_MODEL=kokoro

# Ollama LLM (runs natively, default port 11434)
OLLAMA_BASE_URL=http://localhost:11434/v1
OLLAMA_MODEL=llama3.2:1b

# Vobiz / SIP Trunking Configuration
VOBIZ_SIP_TRUNK_ID=your_trunk_id
OUTBOUND_TRUNK_ID=your_trunk_id
VOBIZ_SIP_DOMAIN=your_sip_domain
VOBIZ_USERNAME=your_username
VOBIZ_PASSWORD=your_password
VOBIZ_OUTBOUND_NUMBER=your_outbound_number
